@model List<TestTable>

@{
    int RowCount = 1;
}
<table class="table table-bordered">
    <tr>
        <td>id</td>
        <td>Name</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
        </tr>
        RowCount++;
    }
</table>